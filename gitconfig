[core]
    editor = vim
    autocrlf = input
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
    conflictstyle = diff3
[mergetool]
    prompt = true
[push]
    default = simple
    ## Simple was introduced in a later version of git.
    #default = current
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
[credential]
	helper = cache --timeout=900
