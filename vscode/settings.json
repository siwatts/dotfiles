            "comments": "#a0a0c0", // Even brighter
            //"comments": "#9595c0",
            "types": {
                "fontStyle": "", // Clear the italics
            },
            "textMateRules": [
                {
                    "scope": "string.quoted.docstring.multi",
                    "settings": {
                        //"foreground": "#8787af",
                        "foreground": "#a0a0c0",
                        //"foreground": "#9595c0",
                        //"fontStyle": "bold"
