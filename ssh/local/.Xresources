! Fonts
xterm*faceName: DejaVu Sans Mono Book
xterm*faceSize: 11

! Load config etc.
xterm*loginshell: false

! Line history
xterm*savelines: 2000

! Scrollbar.
xterm*rightScrollBar: true
xterm*ScrollBar: true

! Scroll to bottom when new lines.
xterm*scrollTtyOutput: false

! Window size.
!xterm*vt100*geometry: 130x50
xterm*vt100*geometry: 100x50
!xterm*vt100*geometry: 80x30

! 256 color mode for vim etc. Can make program specific.
xterm*termName: xterm-256color

! Invert to white on black, if not overriding colors below.
!xterm*reverseVideo: true

! Dracula Xresources palette
xterm*foreground: #F8F8F2
xterm*background: #282A36
xterm*color0: #000000
xterm*color8: #4D4D4D
